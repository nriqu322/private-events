
<div class="section">
    <div class="container">
        <h1 class="title">Log In</h1>
        <%= form_for :session, url:login_path do |f|%>
        <%= f.label :user_id, 'User ID'%>
        <%= f.text_field :user_id, class:'form-control' %>
        <%= f.submit "Submit", class:'btn btn-primary' %>
        <% end %>
    </div>
</div>